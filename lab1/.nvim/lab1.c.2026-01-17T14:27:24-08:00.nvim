// #define_GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(void) {

  char *line = NULL;
  size_t length = 0;
  size_t text;

  printf("Please enter some text: ");

  text = getline(&line, &length, stdin);

  printf("\nTokens:\n");
  char *token;
  char *saveptr = line;

  token = strtok_r(line, " ", &saveptr);
  while (token != NULL) {
    printf("%s \n", token);
  }

  free(line);

  return 0;
}
